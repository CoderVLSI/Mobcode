{
  "expo": {
    "name": "Cursor Chat",
    "slug": "cursor-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "plugins": [
      [
        "llama.rn",
        {
          "forceCxx20": true,
          "enableOpenCLAndHexagon": false
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to photos to attach images in chat."
      }
    },
    "android": {
      "backgroundColor": "#1e1e1e",
      "permissions": [
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.indiccoder.cursorchat"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
